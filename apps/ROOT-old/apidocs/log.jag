                <h3>Log</h3>
<pre class="prettyprint linenums">
&lt%
var log = new Log();
log.info("This is an INFO level log");
%&gt
</pre>
 <% hasMembers = false; %>
<div id="pageLinks">
<a class="navilink" href="#overview">Overview</a>
    <%if(hasMembers) {%>
<a class="navilink" href="#members">Members</a>
